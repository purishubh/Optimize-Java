import java.util.*;
import java.lang.*;

class Main {

	static void calc(int[] arr, int k){
		int n = arr.length; // array length
		int ans = 0;
		int si = 0; // start index
		int ei = k-1; // end index for rach sub array
		while(si<=n-k){ // loop condition, we can put other conditions also like ei<n
			System.out.println("Start Index:"+si+", End Index:"+ei); // print start and end index
			si++; // increment start index;
			ei++; // increment end index;
		}
	}

	public static void main(String args[]) {
		// Your code goes here
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[] arr = new int[n];
		for(int i=0; i<n; i++){
			arr[i] = sc.nextInt();
		}

		int k = sc.nextInt();

		calc(arr, k);
	}
}
